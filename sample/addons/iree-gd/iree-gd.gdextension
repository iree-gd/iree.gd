linux.debug.x86_32 = "libiree-gd.linux.template_debug.x86_32.so"
linux.debug.arm64 = "libiree-gd.linux.template_debug.arm64.so"
linux.debug.arm32 = "libiree-gd.linux.template_debug.arm.so"
macos.debug = "libiree-gd.darwin.template_debug.framework"
windows.debug.x86_64 = "iree-gd.windows.template_debug.x86_64.dll"
windows.debug.x86_32 = "iree-gd.windows.template_debug.x86_32.dll"
android.debug.arm64 = "libiree-gd.android.template_debug.arm64.so"
android.debug.x86_64 = "libiree-gd.android.template_debug.x86_64.so"
