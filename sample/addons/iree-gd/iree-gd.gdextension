windows.release.x86_32 = "iree-gd.windows.template_release.x86_32.dll"
android.release.arm64 = "libiree-gd.android.template_release.arm64.so"
android.release.x86_64 = "libiree-gd.android.template_release.x86_64.so"
ios.release = "libiree-gd.ios.template_release.xcframework"

[dependencies]
ios.debug = {
  "libiree-gd.ios.template_debug.xcframework": ""
}
ios.release = {
  "libiree-gd.ios.template_release.xcframework": ""
}
